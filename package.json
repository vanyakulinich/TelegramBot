{
  "name": "smartreminderbot",
  "version": "1.0.0",
  "description": "'Smart Reminder' Telegram Bot. Reminds all dates, holidays and other things which are set by user",
  "main": "start.js",
  "scripts": {
    "dev": "nodemon start.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-node": "babel-node --presets=env",
    "start": "cross-env NODE_ENV=production node start.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
  "keywords": [
    "reminds",
    "telegram",
    "bot"
  ],
  "author": "Ivan Kulinich",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^5.1.1",
    "firebase": "^5.7.1",
    "firebase-admin": "^6.4.0",
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "node-telegram-bot-api": "^0.30.0",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.21"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "nodemon": "^1.18.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.4.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-merge": "latest",
    "webpack-node-externals": "^1.6.0",
    "rimraf": "^2.6.2"
  }
}
