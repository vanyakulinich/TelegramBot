{
  "name": "smartreminderbot",
  "version": "1.0.0",
  "description": "'Smart Reminder' Telegram Bot. Reminds all dates, holidays and other things which are set by user",
  "main": "start.js",
  "scripts": {
    "dev": "npm run build && nodemon start.js && nodemon --watch src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-node": "babel-node --presets=env",
    "start": "cross-env NODE_ENV=production node start.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
  "keywords": [
    "reminder telegram bot",
    "telegram bot",
    "node",
    "vue ssr"
  ],
  "author": "Ivan Kulinich",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^5.1.1",
    "firebase": "^5.7.3",
    "firebase-admin": "^6.5.0",
    "koa": "^2.6.2",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-static-server": "^1.4.0",
    "node-telegram-bot-api": "^0.30.0",
    "vue": "^2.5.3",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vuetify": "^1.4.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@mdi/font": "^3.3.92",
